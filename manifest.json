{
  "manifest_version": 2,
  "name": "Tab Title Modifier",
  "version": "1.0",
  "description": "A Chrome extension to modify tab titles",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://catalog.grinnell.edu/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
